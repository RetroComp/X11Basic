dim t$(255,11)
read h
for a=0 to 255
  for i=0 to h-1
    read rr$
    t$(a,i)=rr$
  next i
next a

do
lauf$=time$

dim zeile$(11)

for i=0 to len(lauf$)-1
a=peek(varptr(lauf$)+i)
for j=0 to h-1
zeile$(j)=zeile$(j)+t$(a,j)
next j
next i
'for i=0 to h-1
'zeile$(i)=zeile$(i)+zeile$(i)
'next i

'  for k=0 to len(lauf$)*2*6 step 3
    print at(1,1);
    for i=0 to h-1
      print mid$(zeile$(i),k,79)
    next i
    pause 1
'  next k
loop



quit

